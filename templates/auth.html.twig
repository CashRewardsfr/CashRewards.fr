<!DOCTYPE html>
<html lang="fr">

<head>
	<!-- Meta -->
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
	<!-- SITE TITLE -->
	<title>{% block title %}{{ appName }} Authentification {% endblock %}</title>
	<link rel="icon" type="image/png" sizes="16x16" href="{{ asset('themes/front/assets/img/logo.png') }}">
	<!-- Latest Bootstrap min CSS -->
	<link rel="stylesheet" href="{{ asset('themes/front/assets/bootstrap/css/bootstrap.min.css') }}"> <!-- Google Font -->
	<link rel="preconnect" href="https://fonts.gstatic.com">
	<link href="https://fonts.googleapis.com/css2?family=Nunito:wght@400;500;600;700;800&display=swap" rel="stylesheet">
	<!-- Font Awesome CSS -->
	<link rel="stylesheet" href="{{ asset('themes/front/assets/css/all.css') }}">
	<!-- themify-icons CSS -->
	<link rel="stylesheet" href="{{ asset('themes/front/assets/fonts/themify-icons.css') }}">
	<!-- line-icons CSS -->
	<link rel="stylesheet" href="{{ asset('themes/front/assets/fonts/line-icons.css') }}">
	<!--- owl carousel Css-->
	<link rel="stylesheet" href="{{ asset('themes/front/assets/owlcarousel/css/owl.carousel.css') }}">
	<link rel="stylesheet" href="{{ asset('themes/front/assets/owlcarousel/css/owl.theme.css') }}">
	<!-- venobox CSS -->
	<link rel="stylesheet" href="{{ asset('themes/front/assets/css/venobox.css') }}">
	<!-- animate CSS -->
	<link rel="stylesheet" href="{{ asset('themes/front/assets/css/animate.css') }}">
	<!-- MAGNIFIC CSS -->
	<link rel="stylesheet" href="{{ asset('themes/front/assets/css/magnific-popup.css') }}">
	<!-- slicknav CSS -->
	<link rel="stylesheet" href="{{ asset('themes/front/assets/css/slicknav.css') }}">
	<!-- slick CSS -->
	<link rel="stylesheet" href="{{ asset('themes/front/assets/css/slick.css') }}">
	<!-- slick-theme CSS -->
	<link rel="stylesheet" href="{{ asset('themes/front/assets/css/slick-theme.css') }}">
	<!-- Style CSS -->
	<link rel="stylesheet" href="{{ asset('themes/front/assets/css/style.css') }}">
	<link rel="stylesheet" href="{{ asset('themes/front/assets/css/costom.css') }}">
	<!-- responsive CSS -->
	<link rel="stylesheet" href="{{ asset('themes/front/assets/css/responsive.css') }}">
	<!-- switcher CSS -->
	<link rel="stylesheet" href="{{ asset('themes/front/assets/css/switcher/switcher.css') }}">
	<link rel="stylesheet" href="{{ asset('themes/front/assets/css/switcher/style1.css') }}" id="colors">
	<!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
	<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
            <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>

<body data-spy="scroll" data-offset="80" class="bg-white" data-turbo="false">

	<!-- Header Top -->
	<section class="header_top">
		<div class="container">
			<nav class="navbar navbar-expand-md bg-transparent navbar-dark">
				<!-- Brand -->
				<a href="{{ path('app_home') }}" class="navbar-brand logo pt-3" style="font-size: 2rem;">
					<i class="fa fa-hand-holding-usd"></i> <span style="color: #7dcea0;">Cash</span><span style="color: #3498db;">Rewards.fr (autodeploy)</span>
				</a>

				<!-- Toggler/collapsibe Button -->
				<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#collapsibleNavbar">
					<span class="navbar-toggler-icon"></span>
				</button>

				<!-- Navbar links -->
				<div class="collapse navbar-collapse" id="collapsibleNavbar">
					<ul class="navbar-nav ml-auto">
						{% if app.request.attributes.get('_route') == 'app_register' %}

						<li class="nav-item">
							<a href="{{ path('app_user_dashboard') }}" class="btn btn-outline-secondary"><i class="fa fa-user small"></i>Connexion</a>
						</li>

						{% elseif app.request.attributes.get('_route') == 'app_login' %}

						<li class="nav-item">
							<a href="{{ path('app_register') }}" class="btn btn-outline-secondary"><i class="fa fa-key small"></i> S'inscrire</a>
						</li>

						{% endif %}
					</ul>
				</div>
			</nav>
		</div>
	</section>
	<!-- Header Top -->

	<!-- Home Welcome section -->
	<section class="home-section-area">
		<div class="container">
			<div class="row">
				<div class="col-md-6">
					{% block textBloc %}{% endblock %}
				</div>
				<div class="col-md-6 ml-autowow fadeInRight" data-wow-duration="6s" data-wow-delay="0.6s" data-wow-offset="0">

					{% include "widgets/_flashmessage.html.twig" %}

					{% block body %}{% endblock %}

				</div>
			</div>
		</div>
	</section>



	<footer class="footer costom-padding-style-2">
		<div class="container">
			<div class="row">
				<div class="col-sm-12 text-center wow zoomIn">
					<div class="footer_content">
						<p>Â© 2022 Copyright {{ appName }}.fr</p>
					</div>												
				</div><!--- END COL -->
			</div><!--- END ROW -->
		</div><!--- END CONTAINER -->	
	</footer>
	<!-- END FOOTER -->

	<!-- Latest jQuery -->
	<script src="{{ asset('themes/front/assets/js/jquery-1.12.4.min.js') }}"></script>
	<!-- Latest compiled and minified Bootstrap -->
	<script src="{{ asset('themes/front/assets/bootstrap/js/bootstrap.min.js') }}"></script>
	<!-- modernizr JS -->
	<script src="{{ asset('themes/front/assets/js/modernizr-2.8.3.min.js') }}"></script>
	<!-- jquery.nav JS -->
	<script src="{{ asset('themes/front/assets/js/jquery.nav.js') }}"></script>
	<!-- slicknav JS -->
	<script src="{{ asset('themes/front/assets/js/jquery.slicknav.js') }}"></script>
	<!-- stellar js -->
	<script src="{{ asset('themes/front/assets/js/jquery.stellar.min.js') }}"></script>
	<!-- countTo js -->
	<script src="{{ asset('themes/front/assets/js/jquery.inview.min.js') }}"></script>
	<!-- owl-carousel min js  -->
	<script src="{{ asset('themes/front/assets/owlcarousel/js/owl.carousel.min.js') }}"></script>
	<!-- jquery mixitup min js -->
	<script src="{{ asset('themes/front/assets/js/jquery.mixitup.js') }}"></script>
	<!-- jquery venobox min js -->
	<script src="{{ asset('themes/front/assets/js/venobox.min.js') }}"></script>
	<!-- scrolltopcontrol js -->
	<script src="{{ asset('themes/front/assets/js/scrolltopcontrol.js') }}"></script>
	<!-- MAGNIFICANT JS -->
	<script src="{{ asset('themes/front/assets/js/jquery.magnific-popup.min.js') }}"></script>
	<!-- jquery appear js -->
	<script src="{{ asset('themes/front/assets/js/jquery.appear.js') }}"></script>
	<!-- slick js -->
	<script src="{{ asset('themes/front/assets/js/slick.js') }}"></script>
	<!-- WOW - Reveal Animations When You Scroll -->
	<script src="{{ asset('themes/front/assets/js/wow.min.js') }}"></script>
	<!-- switcher js -->
	<script src="{{ asset('themes/front/assets/js/switcher.js') }}"></script>
	<!-- scripts js -->
	<script src="{{ asset('themes/front/assets/js/scripts.js') }}"></script>

	{% block javascripts %}
		{{ encore_entry_script_tags('app') }}
	{% endblock %}
</body>
</html>
